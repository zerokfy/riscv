declare rom_wrap {
    input       addr[32];
    output      rdata[32];
}

module rom_wrap {
    CodeROM     rom;

    {
        rom.clock   = m_clock;
        rom.address = addr[8:0];
        rdata       = rom.q;
    }
}

declare CodeROM interface {
    input         clock
                , address[9]
                ;
    output        q[32]
                ;
}

